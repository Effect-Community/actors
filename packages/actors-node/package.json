{
  "name": "@effect-ts/actors-node",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/Effect-TS/actors.git",
  "homepage": "https://www.matechs.com",
  "private": true,
  "scripts": {
    "clean": "rimraf build tsbuildinfo",
    "build:lib": "ttsc --build  tsconfig.build.json && build-utils package",
    "build:es6": "ttsc --build  tsconfig.es6.json && babel build/esm/ -d build/esm/ -s --plugins annotate-pure-calls && build-utils package-esm",
    "build:post-process": "build-utils rewrite-source-maps && build-utils rewrite-locals && yarn prettier:lib",
    "build": "yarn run build:lib && yarn run build:es6 && yarn run build:post-process",
    "lint": "eslint . --ext .ts,.tsx",
    "autofix": "yarn prettier && yarn lint --fix && yarn prettier",
    "prettier": "prettier --write \"./{src,test,demo}/**/*.ts\"",
    "prettier:lib": "prettier --write \"./build/**/*.js\"",
    "tc": "tsc --noEmit",
    "demo": "ts-node --compiler ttypescript --project tsconfig.build.json -r tsconfig-paths/register -r \"@effect-ts/core/Tracing/Enable\""
  },
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "sideEffects": false,
  "config": {
    "modules": [
      "Cluster",
      "Distributed",
      "Persistence",
      "Remote",
      "ShardContext",
      "Singleton",
      "Transactional"
    ]
  },
  "peerDependencies": {
    "@effect-ts/core": "^0.40.0"
  },
  "dependencies": {
    "@effect-ts/keeper": "^0.0.0",
    "@effect-ts/pg": "^0.0.0",
    "@effect-ts/actors": "^0.0.0"
  }
}
